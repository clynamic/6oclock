            }
          },
          "xl": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DashboardPosition"
            }
          }
        }
      },
      "DashboardConfig": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string",
            "enum": ["moderator", "janitor"]
          },
          "userId": {
            "type": "number"
          },
          "positions": {
            "$ref": "#/components/schemas/DashboardPositions"
          },
          "meta": {
            "type": "object"
          }
        },
        "required": ["id", "type", "userId", "positions", "meta"]
      },
      "DashboardUpdate": {
        "type": "object",
